<header class="navbar navbar-fixed-top navbar-default ">
    <!-- <div class="navbar-inner"> -->
        <div class="container-fluid">

            <div class="navbar-header">
                <%= link_to "Subloom", root_url, id: "logo", class: "navbar-brand"%>
            </div>
            <nav>
            <ul class="nav navbar-nav">
                <li class= "nav navbar-form">
                    <%= render 'search/searchform' %>
                </li>
            </ul>
            <ul class="nav pull-right">
                <li><%= link_to "Home",   root_url %></li>
                <li><%= link_to "Photographers", photogs_index_path %></li>
                <%# If user is logged in, show log out link %>
                <% if current_user.present? %>
                <li><%= link_to 'Profile', profile_path(current_user.profile) %></li>
                <li><%= link_to 'Sign Out', destroy_user_session_path, :method => :delete %></li>
                <%# If user is not logged in, show signup and login links %>
                <% else %>

                <li><%= link_to 'Sign In', new_user_session_path %></li>
                <li><%= link_to 'Register Now!', new_user_registration_path %> </li>
                <% end %>
            </ul>
        </nav>
    </div>
<!-- </div> -->
</header>


